{
  "routes": [
    {
      "route": "/api/todos",
      "methods": [
        "POST",
        "PUT",
        "DELETE"
      ],
      "allowedRoles": [
        "authorised"
      ]
    },
    {
      "route": "/.auth/login/google",
      "statusCode": 404
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "/images/*.{png,jpg,gif}", "/css/*", "/api/*" ]
  }
}
